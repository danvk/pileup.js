{
  "name": "pileup",
  "version": "0.1.4",
  "description": "JavaScript track viewer",
  "keywords": [
    "genome",
    "track",
    "bam",
    "gene",
    "bioinformatics",
    "genomics",
    "sequencing",
    "reads",
    "interactive",
    "biojs"
  ],
  "main": "build/main/pileup.js",
  "browser": "build/main/pileup.js",
  "files": [
    "build",
    "src",
    "style"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hammerlab/pileup.js.git"
  },
  "scripts": {
    "jstransform": "./scripts/jstransform.sh",
    "jstransform-watch": "JSTRANSFORM_OPTS='--watch' ./scripts/jstransform.sh",
    "browserify": "browserify -r .:pileup -v --debug -o build/pileup.browser.js",
    "watch": "watchify -r .:pileup -v --debug -o build/pileup.browser.js",
    "lint": "./scripts/lint.sh",
    "test": "grunt test",
    "flow": "grunt flow",
    "sniper": "biojs-sniper",
    "build": "npm run jstransform && npm run browserify && grunt uglify",
    "install": "npm run build"
  },
  "author": "Dan Vanderkam",
  "contributors": [
    {
      "name": "B. Arman Aksoy",
      "email": "arman@aksoy.org"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hammerlab/pileup.js/issues"
  },
  "homepage": "https://github.com/hammerlab/pileup.js",
  "prepush": [
    "lint"
  ],
  "dependencies": {
    "backbone": "1.1.2",
    "d3": "^3.5.5",
    "jbinary": "^2.1.3",
    "jdataview": "^2.5.0",
    "pako": "^0.2.5",
    "q": "^1.1.2",
    "react": "^0.13.2",
    "shallow-equals": "0.0.0",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "arraybuffer-slice": "^0.1.2",
    "browserify": "^10.2.4",
    "chai": "^2.0.0",
    "coveralls": "2.10.x",
    "es5-shim": "^4.1.0",
    "flow-bin": "^0.12.0",
    "grunt": "^0.4.5",
    "grunt-browserify": "3.8.0",
    "grunt-cli": "^0.1.13",
    "grunt-contrib-connect": "^0.9.0",
    "grunt-contrib-uglify": "^0.8.0",
    "grunt-env": "^0.4.2",
    "grunt-exorcise": "^1.0.0",
    "grunt-flow-type-check": "^0.4.2",
    "grunt-jscoverage": "^0.1.3",
    "grunt-mocha-phantomjs": ">=0.6.1",
    "grunt-mocha-test": "^0.12.7",
    "jstransform": "^11.0.2",
    "jstransformify": "^1.0.0",
    "jsxhint": "git://github.com/strml/JSXHint.git",
    "lcov-parse": "0.0.9",
    "mocha": "^2.1.0",
    "mocha-lcov-reporter": ">=0.0.2",
    "parse-data-uri": "^0.2.0",
    "prepush-hook": "^0.1.0",
    "react-tools": "^0.13.1",
    "reactify": "danvk/reactify",
    "sinon": "^1.12.2",
    "sniper": "^0.2.16",
    "source-map": "^0.3.0",
    "watchify": "^3.2.1"
  },
  "sniper": {
    "js": [
      "/build/pileup.browser.js"
    ],
    "css": [
      "style/pileup.css"
    ],
    "first": "playground"
  }
}
